<!--pages/report/report.weuiml-->
<view class="container">
  <scroll-view class="main-box">
    <view class="weui-cells__title">评估结果</view>
    <view class="weui-cells weui-cells_after-title">
      <view class="weui-cell" wx:for="{{tags}}" wx:key="id">
        <view class="weui-cell__bd">{{item.text}}</view>
      </view>
    </view>
  </scroll-view>
  <view class="bottom-box">
    <view class="actions">
      <button class="weui-btn action-btn" type='primary' bindtap='goIndex'>返回</button>
    </view>
  </view>
</view>